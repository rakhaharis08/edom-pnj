    			'Pertanyaan 18', 'Pertanyaan 19', 'Pertanyaan 20', 'Pertanyaan 21', 'Pertanyaan 22', 'Pertanyaan 23',
    			'Pertanyaan 24', 'Pertanyaan 25', 'Pertanyaan 26', 'Pertanyaan 27'
  			],
  			datasets: [{
    		label: 'List Pertanyaan',
    		backgroundColor: 'rgba(60,141,188,0.9)',
    		borderColor: 'rgba(60,141,188,0.8)',
    		pointRadius: false,
    		pointColor: '#3b8bba',
    		pointStrokeColor: 'rgba(60,141,188,1)',
    		pointHighlightFill: '#fff',
    		pointHighlightStroke: 'rgba(60,141,188,1)',
   			data: [
      			<%= averageData.q1 %>, <%= averageData.q2 %>, <%= averageData.q3 %>, <%= averageData.q4 %>,
      			<%= averageData.q5 %>, <%= averageData.q6 %>, <%= averageData.q7 %>, <%= averageData.q8 %>,
      			<%= averageData.q9 %>, <%= averageData.q10 %>, <%= averageData.q11 %>, <%= averageData.q12 %>,
    			  <%= averageData.q13 %>, <%= averageData.q14 %>, <%= averageData.q15 %>, <%= averageData.q16 %>,
      			<%= averageData.q17 %>, <%= averageData.q18 %>, <%= averageData.q19 %>, <%= averageData.q20 %>,
      			<%= averageData.q21 %>, <%= averageData.q22 %>, <%= averageData.q23 %>, <%= averageData.q24 %>,
      			<%= averageData.q25 %>, <%= averageData.q26 %>, <%= averageData.q27 %>, '1'
    		]
        
  			}]
  };
    
		  var barChartOptions = {
			responsive              : true,
			maintainAspectRatio     : false,
			datasetFill             : false
		  };
		  
		  new Chart(barChartCanvas, {
			type: 'bar',
			data: barChartData,
			options: barChartOptions
		  });
		});
